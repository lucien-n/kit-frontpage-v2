{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	},
	"ts-node": {
		"transpileOnly": true,
		"compilerOptions": {
			"module": "ESNext",
			"esModuleInterop": true
		},
		"esm": true
	}
}
